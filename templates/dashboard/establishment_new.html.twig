{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        <h1>Créer un nouvel établissement</h1>
        {{ form_start(form) }}
            {{ form_row(form.name) }}
            {{ form_row(form.address) }}
            {{ form_row(form.postalCode) }}
            {{ form_row(form.city) }}
            {{ form_row(form.phone) }}
            {{ form_row(form.email) }}
            {{ form_row(form.website) }}
            {{ form_row(form.siret) }}
            {{ form_row(form.create) }}
        {{ form_end(form) }}
    </div>
{% endblock %}